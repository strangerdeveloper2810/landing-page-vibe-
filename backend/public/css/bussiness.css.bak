/* Business Hero Section Styles */
.bussiness-class-1111 {
    position: relative;
    overflow: hidden;
    background: var(--White, rgba(255, 255, 255, 1));
    width: 100%;
    margin: 0;
    padding: 72px 0 96px;
    min-height: 100vh;
}

@media (max-width: 768px) {
    .bussiness-class-1111 {
        padding: 40px 0 60px;
        min-height: auto;
    }
}

/* Auto-align helpers for hero decorative icons */
:root {
    --business-container-max: 1140px;
}

@media (max-width: 1200px) {
    :root {
        --business-container-max: 960px;
    }
}

@media (max-width: 992px) {
    :root {
        --business-container-max: 720px;
    }
}

@media (max-width: 768px) {
    :root {
        --business-container-max: 540px;
    }
}

@keyframes wavePulse {
    0% {
        transform: scale(0.98);
        opacity: 0.22;
    }
    50% {
        transform: scale(1.03);
        opacity: 0.34;
    }
    100% {
        transform: scale(0.98);
        opacity: 0.22;
    }
}

/* Base icon container without hard edges */
#business-hero .icon-base {
    position: absolute;
    width: 100px;
    height: 100px;
    z-index: 0;
    opacity: 1;
    --rot: 0deg;
    will-change: transform;
}

#business-hero .icon-base img {
    width: 100%;
    height: 100%;
    display: block;
}

#business-hero .icon-base::before {
    content: '';
    position: absolute;
    inset: -28px;
    border-radius: 24px;
    background: radial-gradient(60% 60% at 50% 50%, rgba(255, 255, 255, 0.95) 0%, rgba(189, 230, 242, 0.65) 35%, rgba(189, 230, 242, 0.25) 60%, rgba(189, 230, 242, 0) 100%);
    filter: blur(12px);
    pointer-events: none;
    z-index: -1;
}

/* Soft animated light waves */
#business-hero .icon-base::after {
    content: '';
    position: absolute;
    inset: -60px;
    border-radius: 50%;
    background: radial-gradient(70% 70% at 50% 50%, rgba(255, 255, 255, 0.35) 0%, rgba(160, 214, 232, 0.18) 40%, rgba(160, 214, 232, 0) 70%);
    filter: blur(22px);
    pointer-events: none;
    z-index: -2;
    animation: wavePulse 6.5s ease-in-out infinite;
}

#business-hero .icon-checked::after {
    animation-delay: 1.2s;
}

#business-hero .icon-idea {
    top: 53px;
    left: 131px;
    --rot: 10deg;
}

#business-hero .icon-checked {
    top: 255px;
    left: 87px;
    --rot: -10deg;
}

/* Right-side icons */
#business-hero .icon-calendar {
    top: 56px;
    right: 131px;
    --rot: -10deg;
}

#business-hero .icon-folder {
    top: 225px;
    right: 87px;
    --rot: 10deg;
}

#business-hero .icon-calendar img,
#business-hero .icon-folder img {
    box-shadow: none;
    border-radius: 24px;
    background: transparent;
}

/* remove glow layers for these two to avoid any halo/viá»n */
#business-hero .icon-calendar::before,
#business-hero .icon-calendar::after,
#business-hero .icon-folder::before,
#business-hero .icon-folder::after {
    content: none;
}

@media (max-width: 768px) {
    #business-hero .icon-base {
        width: 80px;
        height: 80px;
    }

    #business-hero .icon-base::before {
        inset: -22px;
        filter: blur(10px);
    }

    #business-hero .icon-base::after {
        inset: -46px;
        filter: blur(18px);
        animation-duration: 7.2s;
    }

    #business-hero .icon-idea {
        top: 48px;
        left: 32px;
        --rot: 10deg;
    }

    #business-hero .icon-checked {
        top: 188px;
        left: 12px;
        --rot: -10deg;
    }

    #business-hero .icon-calendar {
        right: 24px;
        top: 40px;
    }

    #business-hero .icon-folder {
        right: 16px;
        top: 190px;
    }
}

/* Chaotic but gentle floating animations for 4 icons */
@keyframes floatChaos1 {
    0% {
        transform: translate(0px, 0px) rotate(var(--rot));
    }
    20% {
        transform: translate(18px, -14px) rotate(calc(var(--rot) + 2deg));
    }
    40% {
        transform: translate(-16px, 18px) rotate(calc(var(--rot) - 2deg));
    }
    60% {
        transform: translate(22px, 12px) rotate(calc(var(--rot) + 3deg));
    }
    80% {
        transform: translate(-14px, -20px) rotate(calc(var(--rot) - 2deg));
    }
    100% {
        transform: translate(0px, 0px) rotate(var(--rot));
    }
}

@keyframes floatChaos2 {
    0% {
        transform: translate(0px, 0px) rotate(var(--rot));
    }
    25% {
        transform: translate(-22px, 12px) rotate(calc(var(--rot) - 3deg));
    }
    50% {
        transform: translate(18px, -20px) rotate(calc(var(--rot) + 2deg));
    }
    75% {
        transform: translate(-14px, 22px) rotate(calc(var(--rot) + 3deg));
    }
    100% {
        transform: translate(0px, 0px) rotate(var(--rot));
    }
}

@keyframes floatChaos3 {
    0% {
        transform: translate(0px, 0px) rotate(var(--rot));
    }
    15% {
        transform: translate(18px, 18px) rotate(calc(var(--rot) + 2deg));
    }
    35% {
        transform: translate(-22px, -14px) rotate(calc(var(--rot) - 3deg));
    }
    65% {
        transform: translate(24px, -18px) rotate(calc(var(--rot) + 2deg));
    }
    90% {
        transform: translate(-18px, 22px) rotate(calc(var(--rot) - 2deg));
    }
    100% {
        transform: translate(0px, 0px) rotate(var(--rot));
    }
}

@keyframes floatChaos4 {
    0% {
        transform: translate(0px, 0px) rotate(var(--rot));
    }
    22% {
        transform: translate(22px, -22px) rotate(calc(var(--rot) + 3deg));
    }
    44% {
        transform: translate(-24px, 12px) rotate(calc(var(--rot) - 2deg));
    }
    66% {
        transform: translate(18px, 24px) rotate(calc(var(--rot) + 2deg));
    }
    88% {
        transform: translate(-14px, -18px) rotate(calc(var(--rot) - 2deg));
    }
    100% {
        transform: translate(0px, 0px) rotate(var(--rot));
    }
}

#business-hero .icon-idea {
    animation: floatChaos1 6.6s ease-in-out infinite alternate;
}

#business-hero .icon-checked {
    animation: floatChaos2 7.1s ease-in-out 0.4s infinite alternate;
}

#business-hero .icon-calendar {
    animation: floatChaos3 6.8s ease-in-out 0.2s infinite alternate;
}

#business-hero .icon-folder {
    animation: floatChaos4 7.4s ease-in-out 0.7s infinite alternate;
}

